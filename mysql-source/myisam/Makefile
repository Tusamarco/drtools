.sh:
	/bin/rm -f $@ $@-t
	/bin/sed \
	  -e 's!@''bindir''@!$(bindir)!g' \
	  -e 's!@''scriptdir''@!$(bindir)!g' \
	  -e 's!@''prefix''@!$(prefix)!g' \
	  -e 's!@''datadir''@!$(datadir)!g' \
	  -e 's!@''localstatedir''@!$(localstatedir)!g' \
	  -e 's!@''libexecdir''@!$(libexecdir)!g' \
	  -e 's!@''CC''@!gcc!'\
	  -e 's!@''CXX''@!g++!'\
	  -e 's!@''GXX''@!yes!'\
	  -e 's!@''PERL''@!/usr/bin/perl!' \
	  -e 's!@''CFLAGS''@! -DDBUG_OFF -g  !'\
	  -e 's!@''CXXFLAGS''@!-O3 -DDBUG_OFF    -fno-implicit-templates -fno-exceptions -fno-rtti!'\
	  -e 's!@''LDFLAGS''@!!'\
	  -e 's!@''VERSION''@!5.0.45!' \
	  -e 's!@''MYSQL_SERVER_SUFFIX''@!!' \
	  -e 's!@''COMPILATION_COMMENT''@!Source distribution!' \
	  -e 's!@''MACHINE_TYPE''@!i686!' \
	  -e 's!@''HOSTNAME''@!/bin/hostname!' \
	  -e 's!@''SYSTEM_TYPE''@!pc-linux-gnu!' \
	  -e 's!@''CHECK_PID''@!/bin/kill -0 $$PID > /dev/null 2> /dev/null!' \
	  -e 's!@''FIND_PROC''@!/bin/ps p $$PID | grep -v grep | grep $$MYSQLD > /dev/null!' \
	  -e 's!@''MYSQLD_DEFAULT_SWITCHES''@!--skip-external-locking!' \
	  -e 's!@''MYSQL_UNIX_ADDR''@!/tmp/mysql.sock!' \
	  -e 's!@''TARGET_LINUX''@!true!' \
	  -e "s!@""CONF_COMMAND""@!./configure  '--without-docs' '--without-man' '--without-bench' '--without-extra-tools' 'CFLAGS=-g' 'CFLAGS= -DDBUG_OFF -g  ' 'CXXFLAGS=-O3 -DDBUG_OFF    -fno-implicit-templates -fno-exceptions -fno-rtti'!" \
	  -e 's!@''MYSQLD_USER''@!mysql!' \
	  -e 's!@''sysconfdir''@!${prefix}/etc!' \
	  -e 's!@''SHORT_MYSQL_INTRO''@!@SHORT_MYSQL_INTRO@!' \
	  -e 's!@''SHARED_LIB_VERSION''@!15:0:0!' \
	  -e 's!@''MYSQL_BASE_VERSION''@!5.0!' \
	  -e 's!@''MYSQL_NO_DASH_VERSION''@!5.0.45!' \
	  -e 's!@''MYSQL_TCP_PORT''@!3306!' \
	  -e 's!@''PERL_DBI_VERSION''@!@PERL_DBI_VERSION@!' \
	  -e 's!@''PERL_DBD_VERSION''@!@PERL_DBD_VERSION@!' \
	  -e 's!@''PERL_DATA_DUMPER''@!@PERL_DATA_DUMPER@!' \
	$< > $@-t
	/bin/chmod +x $@-t
	/bin/mv $@-t $@

